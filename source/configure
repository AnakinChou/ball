				_TMP=`${FIND} $i ${FIND_OPT} ${FIND_ARG} -print 2>/dev/null`
				for j in ${_TMP} ; do
					if test "${_INCLUDES}" = "" ; then
						_INCLUDES=`echo $j|${SED} "s/include\/.\*\$/include/"`
					fi
				done
			done
		fi
	fi

	SIP_INC_PATH="${_INCLUDES}"

	if test "${SIP_INC_PATH}" = "" ; then
		echo "$ac_t""not found!" 1>&6
		echo "$ac_t""" 1>&6
		echo "$ac_t""Please specify the path to the directory that contains" 1>&6
		echo "$ac_t""sip.h using the option --with-sip-incl=DIR." 1>&6
		echo "$ac_t""If you do not have that file, you should obtain SIP" 1>&6
		echo "$ac_t""from" 1>&6
		echo "$ac_t""www.thekompany.com/projects/pykde" 1>&6
		{ echo "configure: error: Aborted." 1>&2; exit 1; }
	else
		echo "$ac_t""${SIP_INC_PATH}" 1>&6
		SIP_INCLUDES="-I${SIP_INC_PATH}"
	fi
fi

