	# Extract the first word of "uname", so it can be a program name with args.
set dummy uname; ac_word=$2
echo $ac_n "checking for $ac_word""... $ac_c" 1>&6
echo "configure:858: checking for $ac_word" >&5
if eval "test \"`echo '$''{'ac_cv_path_UNAME'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  case "$UNAME" in
  /*)
  ac_cv_path_UNAME="$UNAME" # Let the user override the test with a path.
  ;;
  ?:/*)			 
  ac_cv_path_UNAME="$UNAME" # Let the user override the test with a dos path.
  ;;
  *)
  IFS="${IFS= 	}"; ac_save_ifs="$IFS"; IFS=":"
  ac_dummy="$PATH"
  for ac_dir in $ac_dummy; do 
    test -z "$ac_dir" && ac_dir=.
    if test -f $ac_dir/$ac_word; then
      ac_cv_path_UNAME="$ac_dir/$ac_word"
      break
    fi
  done
  IFS="$ac_save_ifs"
  test -z "$ac_cv_path_UNAME" && ac_cv_path_UNAME="no"
  ;;
esac
fi
UNAME="$ac_cv_path_UNAME"
if test -n "$UNAME"; then
  echo "$ac_t""$UNAME" 1>&6
else
  echo "$ac_t""no" 1>&6
fi

	if test $UNAME = no ; then
		echo "$ac_t""" 1>&6
		echo "$ac_t""This script requires the unix command uname, but cannot find it." 1>&6
		echo "$ac_t""Please add the correct path to uname to your \$PATH variable" 1>&6
		echo "$ac_t""and restart configure." 1>&6
		echo "$ac_t""" 1>&6
		{ echo "configure: error: aborted" 1>&2; exit 1; }
		exit
	fi


