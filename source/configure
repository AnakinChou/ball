	${RM}  ${BINFMT}/.Dependencies 2>/dev/null
	${RM}  ${BINFMT}/lib*.objects 2>/dev/null
else
	${RM}  .Dependencies 2>/dev/null
	${RM}  lib*.objects 2>/dev/null
fi

