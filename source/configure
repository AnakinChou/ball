echo $ac_n "checking your OS""... $ac_c" 1>&6
echo "configure:1052: checking your OS" >&5
OS=`${UNAME} -s`
OSREV=`${UNAME} -r`
OSMAJOR=`echo $OSREV|${CUT} -d"." -f1`

BINFMT="${OS}"

if test "$OS" = SunOS ; then
	if test "$OSMAJOR" = 5 ; then
		OS=Solaris
		ARCHITECTURE=`${UNAME} -p`
		BINFMT="${OS}-${OSREV}-${ARCHITECTURE}"
	else
		OS=SunOS
	fi
fi

if test "$OS" = Linux ; then
	PROCESSOR=`${UNAME} -m`
	ARCHITECTURE=unknown
	if test "${PROCESSOR}" = sparc -o "${PROCESSOR}" = SPARC ; then
		ARCHITECTURE=sparc
		BINFMT=Linux-sparc
	fi
	if test `echo $PROCESSOR|${CUT} -c1` = i ; then
		ARCHITECTURE=i386
		BINFMT=Linux-i386
	fi
	if test `echo $PROCESSOR` = alpha ; then
		ARCHITECTURE=alpha
		BINFMT=Linux-alpha
	fi

	if test "${ARCHITECTURE}" = "unknown" ; then
		echo "$ac_t""OS: ${OS} / hardware: ${PROCESSOR}" 1>&6
		echo "$ac_t""Sorry - this architecture is currently not supported..." 1>&6
		{ echo "configure: error: aborted" 1>&2; exit 1; }
	fi
fi

if test "${OS}" = IRIX64 ; then
	OS=IRIX
fi

if test "${OS}" = IRIX ; then
	BINFMT=IRIX-${OSREV}
fi

if test "${OS}" = OSF1 ; then
	BINFMT="OSF1-${OSREV}"
	PROCESSOR=`${UNAME} -m`
	ARCHITECTURE=unknown
	if test `echo $PROCESSOR` = alpha ; then
		ARCHITECTURE=alpha
	fi
fi

if test "`echo $OS | ${CUT} -d_ -f1`" = "CYGWIN" ; then
	OS="CYGWIN"
fi

if test "$OS" != Linux -a "$OS" != Solaris -a "$OS" != IRIX -a  "$OS" != OSF1 -a "$OS" != FreeBSD -a "$OS" != "CYGWIN" ; then
	echo "$ac_t""Sorry - your OS is currently not supported..." 1>&6
	{ echo "configure: error: aborted" 1>&2; exit 1; }
fi

if test "${OS}" = Linux ; then
	cat >> confdefs.h <<\EOF
#define BALL_OS_LINUX LINUX
EOF

fi
if test "${OS}" = Solaris ; then
	cat >> confdefs.h <<\EOF
#define BALL_OS_SOLARIS SOLARIS
EOF

fi
if test "${OS}" = IRIX ; then
	cat >> confdefs.h <<\EOF
#define BALL_OS_IRIX IRIX
EOF

fi
if test "${OS}" = OSF1 ; then
	cat >> confdefs.h <<\EOF
#define BALL_OS_OSF1 OSF1
EOF

fi
if test "${OS}" = FreeBSD ; then
	cat >> confdefs.h <<\EOF
#define BALL_OS_FREEBSD FREEBSD
EOF

fi

if test "$ARCHITECTURE" = sparc ; then
	cat >> confdefs.h <<\EOF
#define BALL_ARCH_SPARC SPARC
EOF

fi
if test "$ARCHITECTURE" = i386 ; then
	cat >> confdefs.h <<\EOF
#define BALL_ARCH_I386 I386
EOF

fi
if test "$ARCHITECTURE" = mips ; then
	cat >> confdefs.h <<\EOF
#define BALL_ARCH_MIPS MIPS
EOF

fi
if test "$ARCHITECTURE" = alpha ; then
	cat >> confdefs.h <<\EOF
#define BALL_ARCH_ALPHA ALPHA
EOF

fi

echo "$ac_t""$OS $OSREV (BINFMT=$BINFMT)" 1>&6

