BALL_INLINE
void Radius::setRadius(const Real r)
  throw()
{
  radius_ = r;
}

BALL_INLINE
Real& Radius::getRadius()
  throw()
{
  return radius_;
}

BALL_INLINE
const Real& Radius::getRadius() const
  throw()
{
  return radius_;
}

BALL_INLINE
void Radius::getRadius(Real& r) const
  throw()
{
  r = radius_;
}
