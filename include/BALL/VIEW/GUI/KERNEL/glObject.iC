BALL_INLINE 
void GLObject::setGLPrimitiveManager
  (const GLPrimitiveManager& GL_primitive_manager)
  throw()
{
  GL_primitive_manager_ = (GLPrimitiveManager*)&GL_primitive_manager;
}

BALL_INLINE 
GLPrimitiveManager* GLObject::getGLPrimitiveManager() const
  throw()
{
  return GL_primitive_manager_;
}
