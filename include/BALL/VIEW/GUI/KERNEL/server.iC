BALL_INLINE
void Server::registerObjectCreator(const ObjectCreator& s)
	throw()
{
  object_creator_ = const_cast<ObjectCreator *>(&s);
}

BALL_INLINE
void Server::unregisterObjectCreator()
	throw()
{
	object_creator_ = (ObjectCreator *)(&RTTI::getDefault<ObjectCreator>());
}


