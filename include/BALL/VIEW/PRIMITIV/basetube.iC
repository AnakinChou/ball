BALL_INLINE
Real BaseTube::getLength() const
	throw()
{
  return (getVertex1() - getVertex2()).getLength();
}

BALL_INLINE
Real BaseTube::getSquareLength() const
	throw()
{
  return (getVertex1() - getVertex2()).getSquareLength();
}
